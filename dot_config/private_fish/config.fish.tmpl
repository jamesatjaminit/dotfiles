if status is-interactive
   alias cat 'bat'
   alias dig 'doggo'
   alias ls 'lsd'
   alias sync-brew 'brew bundle --global --cleanup'
   set -x HOMEBREW_NO_ENV_HINTS true
   set -x EDITOR 'nano'
   set -x VISUAL 'code'
end

fish_add_path /opt/homebrew/bin
fish_add_path /opt/homebrew/opt/node@20/bin
fish_add_path /opt/homebrew/opt/rustup/bin